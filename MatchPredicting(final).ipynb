      "source": [
        "Use a confusion matrix, and add in the percentages so it's easier to read."
      ],
      "metadata": {
        "id": "EbqndEc7F9wv"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.metrics import confusion_matrix\n",
        "\n",
        "# Compute confusion matrix\n",
        "conf_matrix = confusion_matrix(test[\"target\"], preds)\n",
        "\n",
        "# Convert confusion matrix to a DataFrame for better readability\n",
        "confusion_table = pd.DataFrame(\n",
        "    conf_matrix,\n",
        "    index=[\"Actual 0 (Loss)\", \"Actual 1 (Draw)\", \"Actual 2 (Win)\"],\n",
        "    columns=[\"Predicted 0 (Loss)\", \"Predicted 1 (Draw)\", \"Predicted 2 (Win)\"]\n",
        ")\n",
        "\n",
        "# Compute percentages for each row\n",
        "conf_matrix_percentages = (conf_matrix.T / conf_matrix.sum(axis=1)).T * 100\n",
        "\n",
        "# Convert percentages to a DataFrame\n",
        "confusion_table_percentages = pd.DataFrame(\n",
        "    conf_matrix_percentages,\n",
        "    index=[\"Actual 0 (Loss)\", \"Actual 1 (Draw)\", \"Actual 2 (Win)\"],\n",
        "    columns=[\"Predicted 0 (Loss)\", \"Predicted 1 (Draw)\", \"Predicted 2 (Win)\"]\n",
        ")\n",
        "\n",
        "# Combine raw counts and percentages into one table\n",
        "combined_table = confusion_table.astype(str) + \" (\" + confusion_table_percentages.round(1).astype(str) + \"%)\"\n",
        "\n",
        "# Print the combined table\n",
        "print(combined_table)\n",
        "\n",
        "\n"
